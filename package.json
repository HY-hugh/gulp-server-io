{
  "name": "gulp-server-io",
  "version": "0.0.0",
  "description":
    "Create a static server, livereload and a socket.io debugger for your SPA development with gulp",
  "homepage": "https://newbran.ch",
  "author": {
    "name": "NEWBRAN.CH",
    "email": "info@newbran.ch ",
    "url": "https://newbran.ch"
  },
  "files": ["src", "server.js"],
  "main": "index.js",
  "keywords": [
    "node",
    "connect",
    "server",
    "livereload",
    "socket.io",
    "gulp",
    "development",
    "spa"
  ],
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-xo": "^0.19.0",
    "eslint-plugin-prettier": "^2.3.1",
    "husky": "^0.14.3",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0",
    "lint-staged": "^4.3.0",
    "nsp": "^2.8.1",
    "prettier": "^1.7.4"
  },
  "scripts": {
    "prepublishOnly": "nsp check",
    "pretest": "eslint . --fix",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository": "git@github.com:NewbranLTD/gulp-server-io.git",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT",
  "dependencies": {
    "connect": "^3.6.5",
    "supertest": "^3.0.0"
  }
}
